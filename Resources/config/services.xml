<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ibrows_importbundle.annotationreader" class="Ibrows\ImportBundle\Annotation\ImportAnnotationReader">
            <call method="setAnnotationReader">
                <argument type="service" id="annotation_reader" />
            </call>
        </service>

        <service id="ibrows_importbundle.hashgenerator" class="Ibrows\ImportBundle\Generator\ImportHashGenerator">
            <argument type="service" id="ibrows_importbundle.annotationreader" />
        </service>
    </services>
</container>
